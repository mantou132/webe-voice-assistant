{
  "manifest_version": 2,
  "name": "Voice Assistant",
  "description": "Firefox extension voice assistant",
  "version": "0.0.1",
  "default_locale": "en",
  "background": {
    "page": "background/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"]
    }
  ],
  "homepage_url": "https://xianqiao.wang",
  "permissions": [
    "menus",
    "tabs",
    "<all_urls>"
  ]
}
